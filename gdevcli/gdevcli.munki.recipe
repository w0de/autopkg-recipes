<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>ParentRecipe</key>
  <string>com.github.w0de.download.gdevcli</string>
	<key>Description</key>
	<string>Gusto's gdev command line utility.</string>
	<key>Identifier</key>
	<string>com.github.w0de.munki.gdevcli</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>gdevcli</string>
		<key>pkginfo</key>
		<dict>
			<key>unattended_install</key>
			<true/>
			<key>display_name</key>
			<string>gdev (Gusto developer CLI)</string>
			<key>developer</key>
			<string>Ethan Hall</string>
			<key>category</key>
			<string>Engineering</string>
			<key>description</key>
			<string>%release_notes%</string>
			<key>catalogs</key>
			<array>
				<string>import</string>
			</array>
		</dict>
	</dict>
	<key>MiniumumVersion</key>
	<string>1.0</string>
	<key>Process</key>
	<array>
		<dict>
			<key>Processor</key>
			<string>Copier</string>
			<key>Arguments</key>
			<dict>
				<key>source_path</key>
				<string>%pathname%</string>
				<key>destination_path</key>
				<string>%pathname%.pkg</string>
			</dict>
		</dict>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>repo_subdirectory</key>
				<string>utilities</string>
				<key>version_comparison_key</key>
				<string>CFBundleVersion</string>
				<key>pkg_path</key>
        <string>%pathname%.pkg</string>
			</dict>
			<key>Processor</key>
			<string>MunkiImporter</string>
		</dict>
	</array>
</dict>
</plist>
